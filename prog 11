//Program #11: Gallons to Liters Converter
#include <iostream>
using namespace std;

int main(){
    float gallons, liters;
    cout << "Enter the number of gallons: ";
    cin >> gallons;
    liters = gallons * 3.7854;
    cout << gallons << " gallons" << " is equivalent to " << liters << " liters";
}
